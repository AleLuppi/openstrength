{
  "name": "openstrength",
  "version": "0.5.7",
  "description": "Smarter and Better Workout Planning",
  "productName": "OpenStrength",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "serve": "npm run dev",
    "dev": "quasar dev -m pwa",
    "build": "npm run build:pwa",
    "build:spa": "quasar build -m spa",
    "build:ssr": "quasar build -m ssr",
    "build:pwa": "quasar build -m pwa",
    "build:pwa:debug": "quasar build -m pwa -d",
    "release": "npm run build && firebase hosting:channel:deploy",
    "deploy": "npm run build && firebase deploy --only hosting",
    "test": "npm run test:unit",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:ci": "vitest run",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\""
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "axios": "^1.2.1",
    "chart.js": "^4.4.2",
    "firebase": "^10.9.0",
    "mixpanel-browser": "^2.49.0",
    "pinia": "^2.0.11",
    "quasar": "^2.6.0",
    "util": "^0.12.5",
    "vue": "^3.4.0",
    "vue-gtag": "^2.0.1",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.3.1",
    "@quasar/app-vite": "^1.3.0",
    "@quasar/quasar-app-extension-testing-unit-vitest": "^1.0.0",
    "@types/mixpanel": "^2.14.8",
    "@types/mixpanel-browser": "^2.49.0",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@vitest/ui": "^1.3.1",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.2",
    "dotenv": "^16.4.5",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.3.1"
  },
  "overrides": {
    "vite": "^4.0.0",
    "@vitejs/plugin-vue": "^4.0.0"
  },
  "engines": {
    "node": "^18 || ^16 || ^14.19",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
